<?xml version="1.0" encoding="utf-8"?>
<!-- ==== Bare minimum Stop Example ====== 
Eksempelet tar utgangspunkt i offisielle NeTEx-XML eksempler og viser et enkleste type stoppested for buss, uten fasiliteter, i en enveiskjørt gate i Oslo.
    Denne består av:
    - STOP PLACE	ruter:sp:1			Krokstien
	- QUAY			ruter:sp:1:q:1		Krokstien (northbound - one way street)
	Frames / gruppering:
	- SITE FRAME inneholder STOP PLACE og QUAY
-->
<PublicationDelivery
	version="1.0" 
	xmlns="http://www.netex.org.uk/netex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:siri="http://www.siri.org.uk/siri"
	xsi:schemaLocation="http://www.netex.org.uk/netex ../../xsd/NeTEx_publication.xsd">
	<PublicationTimestamp>2016-05-18T15:00:00.0Z</PublicationTimestamp>
	<ParticipantRef>PROVIDER_ID</ParticipantRef>
	<dataObjects>
		<SiteFrame version="any" id="ruter:sf:1">
			<stopPlaces>
				<!--===Stop=== -->
				<!-- Merk: Holdeplass-ID vil komme fra Holdeplassregisteret -->
				<StopPlace version="01" created="2016-05-18T15:00:00.0Z" id="ruter:sp:1">
					<Name lang="no-NO">Krokstien</Name>
					<ShortName lang="no-NO">Krokstien</ShortName>
					<Description lang="no-NO">Krokstien i Ringtjernveien (enveiskjørt)</Description>
					<Centroid>
						<Location>
							<gml:pos srsName="WGS84" srsDimension="2">59.54384 10.51364</gml:pos>
						</Location>
					</Centroid>
					<RoadAddress version="any" id="ruter:ra:ringtjernveien">
						<RoadName>Ringtjernveien</RoadName>
					</RoadAddress>
					<AccessibilityAssessment version="any" id="ruter:aa:1">
						<MobilityImpairedAccess>true</MobilityImpairedAccess>
					</AccessibilityAssessment>
					<TransportMode>bus</TransportMode>
					<StopPlaceType>onstreetBus</StopPlaceType>
					<quays>
						<Quay version="01" created="2016-04-21T09:01:00.0Z" id="ruter:sp:1:q:1">
							<Name lang="no-NO">Krokstien</Name>
							<Description lang="no-NO">Krokstien nordgående</Description>
							<Covered>outdoors</Covered>
							<Lighting>wellLit</Lighting>
							<!-- start REMOVED section --> 
							<!-- Comment: Foreløpig uavklart på hvilket detaljnivå dette skal modelleres i Holdeplassregisteret. Men hvis relevant på sikt skal det legges inn som PLACE EQUIPMENTS -->
							<!--
							<placeEquipments>
								<RubbishDisposalEquipment version="any" id="ruter:rde:bus:01">
									<Description>General waste</Description>
								</RubbishDisposalEquipment>
								<PassengerInformationEquipment version="any" id="ruter:pie:bus:01">
									<Note>Info</Note>
								</PassengerInformationEquipment>
							</placeEquipments>
							-->
							<!-- end REMOVED section -->
							<QuayType>busStop</QuayType>
						</Quay>
					</quays>
				</StopPlace>
			</stopPlaces>
		</SiteFrame>
	</dataObjects>
</PublicationDelivery>

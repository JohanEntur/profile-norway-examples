<?xml version="1.0" encoding="utf-8"?>
<!-- 
==== Minimumseksempel for et stoppested med ett stopp ====== 
Eksempelet tar utgangspunkt i offisielle NeTEx-XML eksempler og viser et enkleste type stoppested for buss, uten fasiliteter, i en enveiskjørt gate i Oslo.
    Denne består av:
    - STOP PLACE	NSR:StopPlace:1			Krokstien
	- QUAY			NSR:StopPlace:1:Quay:1		(uten navn)
	Frames / gruppering:
	- SITE FRAME inneholder STOP PLACE og QUAY
-->
<PublicationDelivery
	version="1.0" 
	xmlns="http://www.netex.org.uk/netex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.netex.org.uk/netex ../../../NeTEx-XML/schema/xsd/NeTEx_publication.xsd">
	<PublicationTimestamp>2017-02-13T15:00:00.0Z</PublicationTimestamp>
	<ParticipantRef>NSR</ParticipantRef>
	<dataObjects>
		<SiteFrame version="any" id="NSR:SiteFrame:1">
			<stopPlaces>
				<!-- ===Stop=== -->
				<!-- Alle objekter skal ha nasjonal ID fra stoppestedsregisteret -->
				<StopPlace version="any" id="NSR:StopPlace:10000000">
					<Name>Krokstien</Name>
					
					
					<!-- Generelt tilgjengelig med rullestol -->
					<AccessibilityAssessment version="any" id="NSR:AccessibilityAssessment:1">
						<MobilityImpairedAccess>true</MobilityImpairedAccess>
						<limitations>
							<!-- Minimumskrav til UU-informasjon -->
							<AccessibilityLimitation>
								<WheelchairAccess>true</WheelchairAccess>
								<StepFreeAccess>true</StepFreeAccess>
							</AccessibilityLimitation>
						</limitations>
					</AccessibilityAssessment>
					<!-- Ingen leskur -->
					<Covered>outdoors</Covered>
					<placeEquipments>
						<TicketingEquipment id="NHR:TicketingEquipment:1" version="any">
							<VehicleModes>rail bus</VehicleModes>
							<NumberOfMachines>4</NumberOfMachines>
							<TicketingFacilityList>ticketOffice mobileTicketing
								ticketMachines</TicketingFacilityList>
							<TicketingServiceFacilityList>reservations purchase</TicketingServiceFacilityList>
						</TicketingEquipment>
						<SanitaryEquipment id="NHR:SanitaryEquipment:1" version="any">

							<SanitaryFacilityList>none</SanitaryFacilityList>
						</SanitaryEquipment>
					</placeEquipments>
					
					
					
<!-- Todo: SLETTMEG!

Hvor legger vi:

luggagelockerfacility - bikeRack
place equipment - cycle parking equipment
-->					
					
					<localServices>
						<LeftLuggageService id="leftluggageservice" version="any">
							<LockerFee>true</LockerFee>
						</LeftLuggageService>
					</localServices>
					<TransportMode>bus</TransportMode>
					<StopPlaceType>onstreetBus</StopPlaceType>
					<quays>
						<Quay version="any" id="NSR:Quay:11111111">
							<Centroid>
								<!-- Alltid WGS84 -->
								<Location srsName="WGS84">
									<Longitude>10.8577903</Longitude>
									<Latitude>59.910579</Latitude>
								</Location>
							</Centroid>
							<Covered>outdoors</Covered>
							<Lighting>wellLit</Lighting>
						</Quay>
					</quays>
					
					
				</StopPlace>
			</stopPlaces>
			
			
			<siteFacilitySets>
				<SiteFacilitySet id="sitefacilityset" version="any">
					
				</SiteFacilitySet>
			</siteFacilitySets>
			
		</SiteFrame>
	</dataObjects>
</PublicationDelivery>

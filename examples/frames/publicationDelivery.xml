<?xml version="1.0" encoding="UTF-8"?>
<PublicationDelivery xmlns="http://www.netex.org.uk/netex"
 xmlns:siri="http://www.siri.org.uk/siri"
 xmlns:gml="http://www.opengis.net/gml/3.2"
 xmlns:core="http://www.govtalk.gov.uk/core"
 xmlns:xlink="http://www.w3.org/1999/xlink"
 xmlns:ifopt="http://www.ifopt.org.uk/ifopt"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.netex.org.uk/netex ../../xsd/NeTEx_publication.xsd">
    <PublicationTimestamp>2016-05-30T12:00:00Z</PublicationTimestamp>
    <ParticipantRef>RUT</ParticipantRef>
    <Description>Generell PublicationDelivery</Description>
	
    <!-- Relevante frames legges under dataObjects, bør legges inn i enklest mulig lesbar rekkefølge -->
    <dataObjects>
        
        <!-- CompositeFrame brukes å samle ulike frames inn i objekt -->
        <CompositeFrame version="any" created="2016-05-30T12:00:00Z" id="NHR:cf:NUMBER">
            <codespaces>
                <!-- Codespace, f.eks. for Nasjonalt Holdeplassregister -->
                <Codespace id="nhr">
                    <Xmlns>NHR</Xmlns>
                    <XmlnsUrl>http://www.rutebanken.no/nasjonltholdeplassregister</XmlnsUrl>
                </Codespace>
            </codespaces>
			
            <frames>
            
                <ResourceFrame id="NHR:resourceframe:NUMBER" version="any">
                    <FrameDefaults>
                        <!-- Alle elementer med id-attributt blir prefixet med dette codespacet (dvs RUT:) -->
                        <DefaultCodespaceRef ref="NHR"/>
                        <DefaultLocale>
                            <TimeZone>CET</TimeZone>
                            <SummerTimeZone>CEST</SummerTimeZone>
                            <!-- Alle tekster er på norsk om ikke eksplisitt spesifisert -->
                            <DefaultLanguage>no</DefaultLanguage>
                        </DefaultLocale>
                        <!-- Alle koordinater leveres i henhold til WGS84, ref. "EPSG:4326" -->
                        <DefaultLocationSystem>EPSG:4326</DefaultLocationSystem>
                    </FrameDefaults>
                    <organisations>
                       <Authority version="any" id="NHR:authority">
                           <CompanyNumber>NUMBER</CompanyNumber>
                           <Name>Nasjonalt holdeplassregister</Name>
                           <LegalName>RUTEBANKEN AS</LegalName>
                           <ContactDetails>
                               <Url>http://www.nasjonalreiseplanlegger.com/</Url>
                           </ContactDetails>
                           <OrganisationType>authority</OrganisationType>
                       </Authority>
                    </organisations>
                </ResourceFrame>
				
                <!-- SiteFrame innholder stops, quays og PoI (point of interest) -->
                <SiteFrame id="NHR:siteframe" version="any">
                    <stopPlaces>
                        <!-- Merk at her brukes det codespace NHR som viser at disse elementene eies av NasjonaltHoldeplassRegister -->
                        <StopPlace version="any" id="NHR:stopplace:NUMBER">
                            <Name>Navn</Name>
                            <quays>
                                <Quay version="any" id="NHR:quay:NUMBER"/>
                            </quays>
                        </StopPlace>
                    </stopPlaces>
                </SiteFrame>
                
                <!-- ServiceFrame definerer en service, som networks, routePoints, routes, lines, schedulesStopPoints, stop assignments og destinationDisplays som brukes av et Service Pattern -->
                <ServiceFrame id="NHR:serviceframe" version="any">
					<!-- Beskrivelse av nettverk, hvis relevant -->
                    <Network version="any" id="NHR:n:NUMBER"/>
                    <!-- Konseptuell kjøreretning -->
                    <directions>
                        <Direction version="any" id="NHR:direction:NUMBER"/>
                    </directions>
                    <!-- Beskrivelse av linjen(e) som leveres i filen -->
                    <routePoints>
                        <RoutePoint version="any" id="NHR:routepoint:NUMBER">
                            <projections>
                                <PointProjection version="any" id="NHR:pontprojection:NUMBER"/>
                            </projections>
                        </RoutePoint>      
                    </routePoints>
                    <!-- Beskrivelse av overordnet rute -->
                    <routes>
                        <Route version="any" id="NHR:route:NUMBER">
                            <Name>NAME</Name>
                            <DirectionRef version="any" ref="NHR:direction:NUMBER"/>
                            <!-- rutepunkter (minimum to) -->
                            <pointsInSequence>
                                <PointOnRoute version="any" id="NHR:pointonroute:NUMBER-1" order="1">
                                    <RoutePointRef version="any" ref="NHR:routepoint:NUMBER"/>
                                </PointOnRoute>
                                <PointOnRoute version="any" id="NHR:pointonroute:NUMBER-2" order="2">
                                    <RoutePointRef version="any" ref="NHR:routepoint:NUMBER"/>
                                </PointOnRoute>
                            </pointsInSequence>
                        </Route>
                    </routes>
                    <!-- Beskrive av linjen ruten tilhører -->
                    <lines>
                        <Line version="any" id="NHR:line:NUMBER">
							<Name>LINJENAVN</Name>
                            <PublicCode>LINJENUMMER/KODE</PublicCode>
                            <OperatorRef ref="NHR:authority"/>
                            <routes>
                                <RouteRef version="any" ref="NHR:route:NUMBER"/>
                            </routes>
                        </Line>
                    </lines>
                    <!-- Skiltkasser -->
                    <destinationDisplays>
                        <DestinationDisplay version="any" id="NHR:destinationdisplay:NAME"/>
                    </destinationDisplays>
                    <!-- Definisjon av rutens stoppunkter -->
					<scheduledStopPoints>
                        <ScheduledStopPoint version="any" id="NHR:scheduledstoppoint:NUMBER"/>
                    </scheduledStopPoints>
                    <!-- Knytting av rutens stoppunkter til fysisk stoppested -->
                    <stopAssignments>
						<PassengerStopAssignment version="any"  id="NHR:passengerstopassignment:NUMBER" order="1">
					        <ScheduledStopPointRef version="any" ref="NHR:scheduledstoppoint:NUMBER"/>
                            <StopPlaceRef version="any" ref="NHR:stopplace:NUMBER"/>
                            <QuayRef version="any" ref="NHR:quay:NUMBER"/>
                        </PassengerStopAssignment>
					</stopAssignments>
                    <!-- Turmønster -->
                    <journeyPatterns>
						<JourneyPattern version="any" id="NRH:journeypattern:NUMBER"/>
                    </journeyPatterns>
                </ServiceFrame>
                
                <!-- ServiceCalendarFrame definerer kalenderobjekter (dager, dagtyper, operasjonsperioder, tidspunkt etc.) -->
                <ServiceCalendarFrame id="NRH:servicecalendarframe" version="any">
					<ServiceCalendar version="any" id="RUT:servicecalendar:NUMBER"/>
                    <dayTypes>
                        <DayType version="any" id="NHR:daytype:NAME"/>
                    </dayTypes>
                </ServiceCalendarFrame>
                
                <!-- TimetableFrame definerer ServiceJourney, dvs avgang(er) for en gitt dag/periode. -->                
                <TimetableFrame id="NHR:timetableframe:NUMBER" version="any">
					<!-- Gyldighetsperiode -->
					<validityConditions>
                        <AvailabilityCondition version="any" id="NHR:availabilitycondition:NUMBER"/>
					</validityConditions>
                    <!-- Turer -->
                    <vehicleJourneys>
                        <ServiceJourney version="any" id="NRH:servicejourney:NUMBER">
							<dayTypes>
								<DayTypeRef ref="NRH:journeypattern:NUMBER"/>
							</dayTypes>
							<JourneyPatternRef ref="NRH:journeypattern:NUMBER"/>
							<!-- turens stopp (minimum to) -->
							<calls>
                                <Call>
                                    <ScheduledStopPointRef version="any" ref="NHR:scheduledstoppoint:NUMBER"/>
                                </Call>
                                <Call>
                                    <ScheduledStopPointRef version="any" ref="NHR:scheduledstoppoint:NUMBER"/>
                                </Call>
                            </calls>
                        </ServiceJourney>
                    </vehicleJourneys>
                </TimetableFrame>
                
            </frames>
            
        </CompositeFrame>
        
    </dataObjects>
</PublicationDelivery>

<?xml version="1.0" encoding="UTF-8"?>
<PublicationDelivery xmlns="http://www.netex.org.uk/netex"
 xmlns:siri="http://www.siri.org.uk/siri"
 xmlns:gml="http://www.opengis.net/gml/3.2"
 xmlns:core="http://www.govtalk.gov.uk/core"
 xmlns:xlink="http://www.w3.org/1999/xlink"
 xmlns:ifopt="http://www.ifopt.org.uk/ifopt"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.netex.org.uk/netex ../../xsd/NeTEx_publication.xsd">
    <PublicationTimestamp>2016-05-30T12:00:00Z</PublicationTimestamp>
    <ParticipantRef>RUT</ParticipantRef>

    <Description>Generell PublicationDelivery</Description>

    <!-- Relevante frames legges under dataObjects, bør legges inn i enklest mulig lesbar rekkefølge -->
    <dataObjects>
        
        <!-- CompositeFrame brukes å samle ulike frames inn i objekt. -->
        <CompositeFrame version="1" created="2016-05-30T12:00:00Z" id="NHR:cf:NUMBER">
            <codespaces>
                <!-- Codespace, f.eks. for Nasjonalt Holdeplassregister -->
                <Codespace id="nhr">
                    <Xmlns>NHR</Xmlns>
                    <XmlnsUrl>http://www.rutebanken.no/nasjonltholdeplassregister</XmlnsUrl>
                </Codespace>
            </codespaces>
            
            <frames>
                <ResourceFrame id="NHR:rf:NUMBER" version="any">
                    <FrameDefaults>
                        <!-- Alle elementer med id-attributt blir prefixet med dette codespacet (dvs RUT:) -->
                        <DefaultCodespaceRef ref="NHR"/>
                        <DefaultLocale>
                            <TimeZone>CET</TimeZone>
                            <SummerTimeZone>CEST</SummerTimeZone>
                            <!-- Alle tekster er på norsk om ikke eksplisitt spesifisert -->
                            <DefaultLanguage>no</DefaultLanguage>
                        </DefaultLocale>
                        <!-- Alle koordinater leveres i henhold til WGS84 -->
                        <!-- TODO: rett opp, skal kanskje ikke med i det hele tatt gitt implisitt krav WGS84 --> 
                        <!--DefaultLocationSystem>EPSG:4326</DefaultLocationSystem-->
                    </FrameDefaults>
                    <organisations>
                       <Authority version="1" id="NHR:authority">
                           <CompanyNumber>NUMBER</CompanyNumber>
                           <Name>Nasjonalt holdeplassregister</Name>
                           <LegalName>RUTEBANKEN AS</LegalName>
                           <ContactDetails>
                               <!-- TODO: Oppdater ift norsk profil -->
                               <Url>http://www.nasjonalreiseplanlegger.com/</Url>
                           </ContactDetails>
                           <OrganisationType>authority</OrganisationType>
                       </Authority>
                    </organisations>
                </ResourceFrame>
                
                <!-- SiteFrame innholder stops, quays og PoI (point of interest) -->
                <SiteFrame id="NHR:site" version="any">
                    <stopPlaces>
                        <!-- Merk at her brukes det codespace NHR som viser at disse elementene eies av NasjonaltHoldeplassRegister -->
                        <StopPlace version="1" id="NHR:stop:NUMBER">
                            <Name>Navn</Name>
                            <quays>
                                <Quay version="1" id="NHR:quay:NUMBER"/>
                            </quays>
                        </StopPlace>
                    </stopPlaces>
                </SiteFrame>
                
                <!-- ServiceCalendarFrame definerer kalenderobjekter (dager, dagtyper, operasjonsperioder, tidspunkt etc.) -->
                <ServiceCalendarFrame id="NRH:scf" version="any">
                    <dayTypes>
                        <DayType version="any" id="NHR:dt:day"/>
                    </dayTypes>
                </ServiceCalendarFrame>
                
                <!-- ServiceFrame definerer en service, som networks, routePoints, routes, lines, schedulesStopPoints, stop assignments og destinationDisplays som brukes av et Service Pattern -->
                <ServiceFrame id="sf:rutebanken" version="any">
                    <servicePatterns>
                        <ServicePattern version="any" id="NRH:sp:NUMBER"/>
                    </servicePatterns>
                </ServiceFrame>
                
                <!-- TimetableFrame definerer ServiceJourney, dvs avgang(er) for en gitt dag/periode. -->                
                <TimetableFrame id="NHR:tf:NUMBER" version="any">
                    <!-- vil som oftest inneholde 'vehicleJourney' -->
                    <vehicleJourneys>
                        <ServiceJourney version="any" id="NRH:sj:NUMBER"/>
                    </vehicleJourneys>
                </TimetableFrame>
                
            </frames>
        </CompositeFrame>
    </dataObjects>
</PublicationDelivery>
